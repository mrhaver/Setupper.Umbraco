<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Version>10.0.0</Version>
    <PackageProjectUrl>https://github.com/mrhaver/Setupper.Umbraco.git</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mrhaver/Setupper.Umbraco.git</RepositoryUrl>
    <Authors>mrhaver</Authors>
    <Description>To make life easier</Description>
  </PropertyGroup>

  <ItemGroup>
    <None Remove=".gitignore" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Umbraco.Cms.Core" Version="10.2.1" />
    <PackageReference Include="Umbraco.Cms.Infrastructure" Version="10.2.1" />
    <PackageReference Include="Umbraco.Cms.Web.Common" Version="10.2.1" />
  </ItemGroup>

</Project>
